<!--Schema Markup-->
 <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "Article",
    "headline": "Gafas de sol",
    "datePublished": "2024-01-10",
    "author": {
      "@type":"Person",
      "name": "Julio Infante"
    },
    "publisher":{
      "@type": "Organization",
      "name": "gafas para el sol",
      "logo": {
        "@type": "ImageObject",
        "url": "https://images.pexels.com/photos/343720/pexels-photo-343720.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=1"
      }
    },
    "image": {
      "@type": "ImageObject",
      "url": "https://images.pexels.com/photos/343720/pexels-photo-343720.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=1",
      "width": "800",
      "height": "600"
    },
    "description": "Ventas lentes de sol"
  }
  </script>
	<style type="text/css">
	 @font-face{
            font-family:'fuenteC';
            src:url(/fuentes/HyperSuperFast.ttf);
           }
           body{
           background-image:url('https://images.pexels.com/photos/4904563/pexels-photo-4904563.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=1');
        background-attachment: fixed;
        background-size: cover;
           }
		img{
			width:25%;
			height: auto;
			margin:20px;
		}
		#contenedor{
			display:flex;
			justify-content: center;
		 width: 80%;
		 margin:70px auto;
		}
		h1{
			text-align: center;
			line-height:100px;
			font:50px 'fuenteC';
			color:white;
		}
		a{
			text-decoration:none;
			color:white;
			font:30px 'fuenteC';
			margin-left:20px;
		}
		 @media screen and (max-width: 720px){
		 	img{
			width:70%;
			height: auto;
			margin:20px;
		}
		 }
	</style>
</head>
<body>
  <h1>Imagenes del Producto</h1>

   <div id='contenedor'>
  <% for(let m = 0;m < imagenes.length;m++){ %>
   <img src="<%= imagenes[m].url%>">
  <% } %>

   </div>
   <a href="/clientes">Volver</a>

</body>
</html>